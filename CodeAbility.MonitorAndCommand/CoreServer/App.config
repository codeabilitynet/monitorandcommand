<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings>
    <add key="PortNumber" value="11000"/>
    <add key="HeartbeatPeriod" value="5000"/>
    <add key="IsMessageServiceActivated" value="false"/>
  </appSettings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/>
  </startup>
  <system.serviceModel>
    <bindings>
      <basicHttpBinding>
        <binding name="BasicHttpBinding_IMessageService" />
      </basicHttpBinding>
    </bindings>
    <client>
      <endpoint address="http://localhost:1304/MessageService.svc"
        binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IMessageService"
        contract="MessageServiceReference.IMessageService" name="BasicHttpBinding_IMessageService" />
    </client>
  </system.serviceModel>
  <system.diagnostics>
    <trace autoflush="true" indentsize="4">
      <listeners>
        <add name="TextWriterTraceListener" type="System.Diagnostics.TextWriterTraceListener" initializeData="TextWriterOutput.log" />
        <add name="ConsoleTraceListener" type="System.Diagnostics.ConsoleTraceListener" />
        <remove name="Default" />
      </listeners>
    </trace>
  </system.diagnostics>
</configuration>
